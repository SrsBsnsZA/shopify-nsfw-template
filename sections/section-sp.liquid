{{ 'section-sp.css' | asset_url | stylesheet_tag }}
{% render 'section-style' %}

<div class="section-sp">
  <div class="{%- if section.settings.fullwidth -%}container-fluid{%- else -%}container{%- endif -%}">
    {%- if section.settings.heading != blank -%}
    <h2 class="section-header">{{ section.settings.heading  }}</h2>
    {%- endif -%}
    <p class="section-text">{{ section.settings.text_decoration }}</p>

    <div class="row wp_section">
      {%- for block in section.blocks -%}
        {%- case block.settings.width -%}
        {%- when '25%' -%}
        {%- assign block_width = 'col-lg-3 col-md-3' -%}
        {%- when '33%' -%}
        {%- assign block_width = 'col-xl-4 col-lg-5 col-md-5' -%}
        {%- when '50%' -%}
        {%- assign block_width = 'col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 Ap_pd' -%}
        {%- when '66%' -%}
        {%- assign block_width = 'col-xl-8 col-lg-7 col-md-7' -%}
        {%- when '75%' -%}
        {%- assign block_width = 'col-lg-9 col-md-9' -%}
        {%- when '100%' -%}
        {%- assign block_width = 'col-12' -%}
        {%- endcase -%}
        <div class="{{block_width}}">
          {%- case block.type -%}
          
              {%- when 'image' -%}
              <div class="advanced-image">
                  <div class="advanced-image-2-content">
                  {%- if block.settings.image != blank -%}
                    <div class="no-bg advanced-image-{% if block.settings.effects_image %}blog{% else %}media{% endif %} {% if block.settings.image != blank %}media{% else %}image-advanced__media--placeholder placeholder{% endif %}"
                     style="padding-bottom: {{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100 }}%;"
                    >
                      <img class="advanced__image"
                      srcset="{%- if block.settings.image.width >= 165 -%}{{ block.settings.image | img_url: '165x' }} 165w,{%- endif -%}
                        {%- if block.settings.image.width >= 360 -%}{{ block.settings.image | img_url: '360x' }} 360w,{%- endif -%}
                        {%- if block.settings.image.width >= 535 -%}{{ block.settings.image | img_url: '535x' }} 535w,{%- endif -%}
                        {%- if block.settings.image.width >= 600 -%}{{ block.settings.image | img_url: '850x' }} 850w,{%- endif -%}
                        {%- if block.settings.image.width >= 1070 -%}{{ block.settings.image | img_url: '1070x' }} 1070w,{%- endif -%}
                        {%- if block.settings.image.width >= 1500 -%}{{ block.settings.image | img_url: '1500x' }} 1500w,{%- endif -%}"
                      src="{{ block.settings.image | img_url: 'master' }}"
                      sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
                      alt="{{ block.settings.image.alt | escape }}"
                      loading="lazy"
                      width="{{ block.settings.image.width }}"
                      height="{{ block.settings.image.height }}"
                    >     
                    <div class="wp_img">
                      {%- if block.settings.image_child1 != blank -%}
                            <img class="image_child1"
                            srcset="{%- if block.settings.image_child1.width >= 165 -%}{{ block.settings.image_child1 | img_url: '165x' }} 165w,{%- endif -%}
                              {%- if block.settings.image_child1.width >= 200 -%}{{ block.settings.image_child1 | img_url: '360x' }} 360w,{%- endif -%}
                              {%- if block.settings.image_child1.width >= 535 -%}{{ block.settings.image_child1 | img_url: '535x' }} 535w,{%- endif -%}
                              {%- if block.settings.image_child1.width >= 850 -%}{{ block.settings.image_child1 | img_url: '850x' }} 850w,{%- endif -%}
                              {%- if block.settings.image_child1.width >= 1070 -%}{{ block.settings.image_child1 | img_url: '1070x' }} 1070w,{%- endif -%}
                              {%- if block.settings.image_child1.width >= 1500 -%}{{ block.settings.image_child1 | img_url: '1500x' }} 1500w,{%- endif -%}"
                            src="{{ block.settings.image_child1 | img_url: 'master' }}"
                            sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
                            alt="{{ block.settings.image_child1.alt | escape }}"
                            loading="lazy"
                            width="{{ block.settings.image_child1.width }}"
                            height="{{ block.settings.image_child1.height }}"
                          >
                      {%- endif -%}

                      {%- if block.settings.image_child2 != blank -%}
                        <img class="image_child2"
                        srcset="{%- if block.settings.image_child2.width >= 165 -%}{{ block.settings.image_child2 | img_url: '165x' }} 165w,{%- endif -%}
                          {%- if block.settings.image_child2.width >= 200 -%}{{ block.settings.image_child2 | img_url: '360x' }} 360w,{%- endif -%}
                          {%- if block.settings.image_child2.width >= 535 -%}{{ block.settings.image_child2 | img_url: '535x' }} 535w,{%- endif -%}
                          {%- if block.settings.image_child2.width >= 850 -%}{{ block.settings.image_child2 | img_url: '850x' }} 850w,{%- endif -%}
                          {%- if block.settings.image_child2.width >= 1070 -%}{{ block.settings.image_child2 | img_url: '1070x' }} 1070w,{%- endif -%}
                          {%- if block.settings.image_child2.width >= 1500 -%}{{ block.settings.image_child2 | img_url: '1500x' }} 1500w,{%- endif -%}"
                        src="{{ block.settings.image_child2 | img_url: 'master' }}"
                        sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
                        alt="{{ block.settings.image_child2.alt | escape }}"
                        loading="lazy"
                        width="{{ block.settings.image_child2.width }}"
                        height="{{ block.settings.image_child2.height }}"
                        >
                      {%- endif -%}

                    </div>               
                    </div>
                  {%- else -%}
                      {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                  {%- endif -%}
                  </div>
              </div>

              {%- when 'collection'-%}
              <div class="section-sp-card {% if block.settings.image_collection != blank %}media{% else %}image-advanced__media--placeholder placeholder{% endif %}" style="padding-bottom: {{ 1 | divided_by: block.settings.image_collection.aspect_ratio | times: 100 }}%;" >
                <a href="{{ block.settings.collection.url | default:'#' }}">
                  {%- if block.settings.image_collection != blank -%}
                  <img
                      srcset="{%- if block.settings.image_collection.width >= 165 -%}{{ block.settings.image_collection | img_url: '165x' }} 165w,{%- endif -%}
                        {%- if block.settings.image_collection.width >= 360 -%}{{ block.settings.image_collection | img_url: '360x' }} 360w,{%- endif -%}
                        {%- if block.settings.image_collection.width >= 535 -%}{{ block.settings.image_collection | img_url: '535x' }} 535w,{%- endif -%}
                        {%- if block.settings.image_collection.width >= 750 -%}{{ block.settings.image_collection | img_url: '750x' }} 750w,{%- endif -%}
                        {%- if block.settings.image_collection.width >= 1070 -%}{{ block.settings.image_collection | img_url: '1070x' }} 1070w,{%- endif -%}
                        {%- if block.settings.image_collection.width >= 1500 -%}{{ block.settings.image_collection | img_url: '1500x' }} 1500w,{%- endif -%}"
                      src="{{ block.settings.image_collection | img_url: 'master' }}"
                      sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 100 | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
                      alt="{{ block.settings.image_collection.alt | escape }}"
                      loading="lazy"
                      width="{{ block.settings.image_collection.width }}"
                      height="{{ block.settings.image_collection.height }}"
                    >
                  <h3 class="title-collection">{{ block.settings.collection.title | escape }}</h3>
                  {%- else -%}
                  {{ 'collection-1' | placeholder_svg_tag: 'placeholder-svg' }}
                  <h3 class="collection-title">Collection</h3>
                  {%- endif -%}
                </a>
              </div>


              {%- when 'product'-%}
              <div class="advanced-product">
                <div  class="advanced-product-carousel apslick"
                data-setting='{"accessibility": false, "arrows": {{ block.settings.arrows }},
                "dots": {{ block.settings.dots }}, "draggable": {{ block.settings.draggable }},
                "infinite": {{ block.settings.infinite }}, "autoplay": {{ block.settings.autoplay }},
                "autoplaySpeed": {{ block.settings.autoplaySpeed }}, "speed": {{ block.settings.speed }},
                "slidesToShow": {{ block.settings.slidesToShow }}, "slidesToScroll": {{ block.settings.slidesToScroll }},
                "vertical": false, "mobileFirst": false, "pauseOnHover": {{ block.settings.pauseOnHover }},
                "rows": {{ block.settings.rows }}, "slidesPerRow": 1, "rtl": 1,
                "slidesToShow_992": {{ block.settings.slidesToShow_992 }}, "slidesToScroll_992": {{ block.settings.slidesToScroll_992 }},
                "arrows_992": {{ block.settings.arrows_992 }}, "dots_992": {{ block.settings.dots_992 }},
                "slidesToShow_767": {{ block.settings.slidesToShow_767 }}, "slidesToScroll_767": {{ block.settings.slidesToScroll_767 }},
                "arrows_767": {{ block.settings.arrows_767 }}, "dots_767": {{ block.settings.dots_767 }},
                "slidesToShow_450": {{ block.settings.slidesToShow_450 }}, "slidesToScroll_450": {{ block.settings.slidesToScroll_450 }},
                "arrows_450": {{ block.settings.arrows_450 }}, "dots_450": {{ block.settings.dots_450 }}}'>
                    {%- for product in block.settings.collection.products -%}
                      <div class="items-product">
                        {%- if block.settings.collection.products.size == 1 -%}
                        {% render 'product-card',
                          product_card_product: product,
                          media_size: block.settings.image_ratio,
                          show_secondary_image: block.settings.show_secondary_image,
                          add_image_padding: block.settings.add_image_padding,
                          show_vendor: block.settings.show_vendor
                        %}
                        {%- else -%}
                        {% render 'product-card',
                          product_card_product: product,
                          media_size: block.settings.image_ratio,
                          show_secondary_image: block.settings.show_secondary_image,
                          add_image_padding: block.settings.add_image_padding,
                          show_vendor: block.settings.show_vendor
                        %}
                        {%- endif -%}
                      </div>
                    {%- endfor -%}
                    {%- if block.settings.collection == blank -%}
                        {%- for i in (1..6) -%}
                        <li class="product-items">
                          {%- capture current -%}{%- cycle 1, 2, 3, 4, 5, 6 -%}{%- endcapture -%}
                          {{ 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
                        </li>
                        {%- endfor -%}
                    {%- endif -%}
                </div>
              </div>

              {%- when 'text_list'-%}
              <div class="wp_advanced-text">
                <div class="section-sp_text text-{%- if block.settings.text_align == 'left' -%}start{%- elsif block.settings.text_align == 'center' -%}center{%- elsif block.settings.text_align == 'right' -%}end{%- endif -%}">
                    <h5>{{ block.settings.header_top }}</h5>
                    <h3 class="section-sp_text-title">{{ block.settings.header__text  }}</h3>
                    <p class="section-sp_text-content">{{ block.settings.text__content }}</p>
                    {%- if block.settings.btn_text != blank -%}
                    <a class="btn--main btn-advanced" href="{{ block.settings.btn_link }}" title="{{ block.settings.btn_text }}">{{ block.settings.btn_text }}
                      <i class="far fa-arrow-up"></i>
                    </a>
                    {%- endif -%}
                </div>
                <div class="big_text">{{ block.settings.big_text }}</div>
              </div>   
          
          {%- when 'html'-%}
            {{ block.settings.html__text }}
              
          {%- endcase -%}
        </div>
      {%- endfor -%}
    </div>
  </div>
</div>
